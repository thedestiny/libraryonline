    <listener-class>com.listener.it.ApplicationListener</listener-class>
  </listener>
  <context-param>
    <param-name>dburl</param-name>
    <param-value>jdbc:mysql:///mylibrary</param-value>
  </context-param>
  <context-param>
    <param-name>dbuser</param-name>
    <param-value>root</param-value>
  </context-param>
  <context-param>
    <param-name>dbpwd</param-name>
    <param-value>lky86</param-value>
  </context-param>
  <filter>
    <filter-name>FilterLogin</filter-name>
    <filter-class>com.filter.it.FilterLogin</filter-class>
    <init-param>
      <param-name>uris</param-name>
      <param-value>/login,/index,/custom</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>FilterLogin</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>FilterEcode</filter-name>
    <filter-class>com.filter.it.FilterEcode</filter-class>
    <init-param>
      <param-name>code</param-name>
      <param-value>utf-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>FilterEcode</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
</web-app>