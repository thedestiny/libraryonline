    <servlet-class>com.login.it.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Custom</servlet-name>
    <servlet-class>com.login.it.Custom</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Custom</servlet-name>
    <url-pattern>/custom</url-pattern>
