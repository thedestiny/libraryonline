    <servlet-class>com.book.it.BookEdit</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BookEdit</servlet-name>
    <url-pattern>/bookedit</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BookDel</servlet-name>
    <servlet-class>com.book.it.BookDel</servlet-class>
