    <servlet-name>BookReturn</servlet-name>
    <servlet-class>com.book.it.BookReturn</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BookReturn</servlet-name>
    <url-pattern>/bookreturn</url-pattern>
  </servlet-mapping>
