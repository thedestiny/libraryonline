  </servlet-mapping>
  <servlet>
    <servlet-name>BookAdd</servlet-name>
    <servlet-class>com.book.it.BookAdd</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BookAdd</servlet-name>
    <url-pattern>/bookadd</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BookBorrow</servlet-name>
    <servlet-class>com.book.it.BookBorrow</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BookBorrow</servlet-name>
    <url-pattern>/bookborrow</url-pattern>
  </servlet-mapping>
